<h1>Listing admin_companies</h1>

<table>
  <tr>
    <th>회사명</th>
    <th></th>
  </tr>

<% @companies.each do |company| %>
  <tr>
    <td><%= company.name %></td>
    <td>
      <%= link_to 'Edit', edit_admin_company_path(company) %>
      <%= link_to 'Destroy', admin_company_path(company), :confirm => 'Are you sure?', :method => :delete %>
    </td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Company', new_admin_company_path %>
