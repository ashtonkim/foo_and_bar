<h1>Listing admin_videos</h1>

<table>
  <tr>
    <th>Title</th>
    <th>URL</th>
    <th></th>
  </tr>

<% @videos.each do |video| %>
  <tr>
    <td><%= video.title %></td>
    <td><%= video.stream_url %></td>
    <td>
      <%= link_to 'Show', [:admin, video] %>
      <%= link_to 'Edit', edit_admin_video_path(video) %>
      <%= link_to 'Destroy', [:admin, video], :confirm => 'Are you sure?', :method => :delete %>
    </td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Video', new_admin_video_path %>
