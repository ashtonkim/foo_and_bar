<h1>Listing admin_cars</h1>

<table>
  <tr>
    <th>차명</th>
    <th>가격</th>
    <th></th>
  </tr>

<% @cars.each do |car| %>
  <tr>
    <td><%= car.name %></td>
    <td><%= car.price %></td>
    <td>
      <%= link_to 'Show', [:admin, car] %>
      <%= link_to 'Edit', edit_admin_car_path(car) %>
      <%= link_to 'Destroy', [:admin, car], :confirm => 'Are you sure?', :method => :delete %>
    </td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Car', new_admin_car_path %>
