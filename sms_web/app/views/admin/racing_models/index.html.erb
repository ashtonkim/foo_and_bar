<h1>Listing admin_racing_models</h1>

<table>
  <tr>
    <th>이름</th>
    <th>사진</th>
    <th></th>
  </tr>

<% @racing_models.each do |racing_model| %>
  <tr>
    <td><%= racing_model.name %></td>
    <td><%= image_tag racing_model.image_raw.url(:thumb, false) %></td>
    <td>
      <%= link_to 'Show', [:admin, racing_model] %>
      <%= link_to 'Edit', edit_admin_racing_model_path(racing_model) %>
      <%= link_to 'Destroy', [:admin, racing_model], :confirm => 'Are you sure?', :method => :delete %>
    </td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Racing model', new_admin_racing_model_path %>
